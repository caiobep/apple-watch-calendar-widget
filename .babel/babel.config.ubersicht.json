{
  "presets": ["@babel/typescript"],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-syntax-jsx",
    "@babel/plugin-transform-typescript",
    [
      "babel-plugin-transform-remove-imports",
      {
        "test": "react"
      }
    ]
  ],
  "ignore": ["src/**/*.test.*", "build"]
}
