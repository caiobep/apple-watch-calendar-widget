{
  "name": "apple-watch-calendar",
  "version": "0.0.0",
  "description": "An [Übersicht](http://tracesof.net/uebersicht/) widget simulating the apple watch calendar watchface",
  "scripts": {
    "dev": "npm run build -- --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "babel --config-file ./babel.ubersitch-config.json src --out-dir build --extensions \".ts,.tsx\" --out-file-extension '.jsx'",
    "build:dir": "babel --config-file ./babel.ubersitch-config.json src --out-dir build --extensions \".ts,.tsx\" --out-file-extension '.jsx'",
    "test:watch": "jest -c ./jest.config.json --watchAll",
    "test": "jest -c ./jest.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caiobep/apple-watch-calendar.git"
  },
  "keywords": [],
  "author": "Caio Amaral Corrêa <caiobep@me.com> (https://github.com/caiobep)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caiobep/apple-watch-calendar/issues"
  },
  "homepage": "https://github.com/caiobep/apple-watch-calendar#readme",
  "devDependencies": {
    "@babel/cli": "~7.11.5",
    "@babel/core": "~7.11.5",
    "@babel/plugin-transform-typescript": "~7.11.0",
    "@babel/preset-env": "~7.11.5",
    "@babel/preset-react": "~7.10.4",
    "@babel/preset-typescript": "~7.9.0",
    "@commitlint/cli": "~8.3.5",
    "@commitlint/config-conventional": "~8.3.4",
    "@testing-library/jest-dom": "~5.11.4",
    "@testing-library/react": "~10.4.9",
    "@types/jest": "~25.2.3",
    "@types/react": "~16.9.49",
    "babel-core": "~7.0.0-bridge.0",
    "babel-jest": "~26.0.1",
    "eslint": "~7.0.0",
    "eslint-plugin-jest-dom": "~3.2.2",
    "eslint-plugin-prettier": "~3.1.3",
    "eslint-plugin-testing-library": "~3.8.0",
    "husky": "~4.2.5",
    "jest": "~26.0.1",
    "prettier": "~2.0.5",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "semantic-release": "~17.0.7"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "~7.10.4",
    "babel-plugin-transform-remove-imports": "~1.4.1"
  }
}
